{
  "compilerOptions": {
    "target": "ES2021",
    "importHelpers": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "build",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "types": ["node"],
    "lib": ["ES2022", "DOM"],
    "declaration": false,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "rootDir": ".",
    "allowJs": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "paths": {
      "@aiokit/async-storage": ["libs/async-storage/src/index.ts"],
      "@aiokit/auth": ["libs/auth/src/index.ts"],
      "@aiokit/bootstrap": ["libs/bootstrap/src/index.ts"],
      "@aiokit/common-types": ["libs/common-types/src/index.ts"],
      "@aiokit/config": ["libs/config/src/index.ts"],
      "@aiokit/crypto": ["libs/crypto/src/index.ts"],
      "@aiokit/exceptions": ["libs/exceptions/src/index.ts"],
      "@aiokit/i18n": ["libs/i18n/src/index.ts"],
      "@aiokit/logger": ["libs/logger/src/index.ts"],
      "@aiokit/persistence-api": ["libs/persistence-api/src/index.ts"],
      "@aiokit/server-http-client": ["libs/server-http-client/src/index.ts"],
      "@aiokit/string-utils": ["libs/string-utils/src/index.ts"],
      "@aiokit/swagger-utils": ["libs/swagger-utils/src/index.ts"],
      "@aiokit/test-utils": ["libs/test-utils/src/index.ts"],
      "@aiokit/typeorm": ["libs/typeorm/src/index.ts"],
      "@aiokit/validation": ["libs/validation/src/index.ts"]
    }
  }
}
